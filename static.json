{
    "root": "public/",
    "routes": {
      "/.well-known/apple-app-site-association": {
        "headers": {
          "Content-Type": "application/json"
        },
        "file": "/.well-known/apple-app-site-association"
      },
      "/EN": "EN/index.html",
      "/EN/": "EN/index.html",
      "/en": "EN/index.html",
      "/en/": "EN/index.html",
      "/**": "index.html"
    }
  }